{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": [
    "node_modules",
    "**/*.d.ts",
    "!**/*"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.json"],
      "parser": "jsonc-eslint-parser",
      "rules": {
        "@nx/dependency-checks": "error"
      }
    }
  ],
  "rules": {
    "@typescript-eslint/no-duplicate-enum-values": "off"
  }
}
